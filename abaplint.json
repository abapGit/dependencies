{
  "global": {
    "files": "/src/**/*.*",
    "skipGeneratedGatewayClasses": true,
    "skipGeneratedPersistentClasses": true,
    "skipGeneratedFunctionGroups": true
  },
  "dependencies": [
    {
      "url": "https://github.com/abaplint/deps",
      "folder": "/deps1",
      "files": "/src/**/*.*"
    },
    {
      "url": "https://github.com/larshp/abapGit",
      "folder": "/deps2",
      "files": "/src/**/*.*"
    }
  ],
  "syntax": {
    "version": "v740sp08",
    "errorNamespace": "^(Z|Y|LT?CL_|TY_|LIF_)",
    "globalConstants": [],
    "globalMacros": []
  },
  "rules": {
    "abapdoc": false,
    "allowed_object_naming": true,
    "allowed_object_types": false,
    "ambiguous_statement": true,
    "begin_end_names": true,
    "chain_mainly_declarations": true,
    "check_abstract": true,
    "check_comments": false,
    "check_ddic": true,
    "check_include": true,
    "check_no_handler_pragma": true,
    "check_text_elements": true,
    "check_transformation_exists": true,
    "downport": false,
    "empty_structure": true,
    "forbidden_identifier": true,
    "forbidden_void_type": true,
    "form_no_dash": true,
    "form_tables_obsolete": true,
    "implement_methods": true,
    "in_statement_indentation": false,
    "keep_single_parameter_on_one_line": false,
    "keyword_case": true,
    "local_testclass_location": true,
    "main_file_contents": true,
    "prefer_inline": false,
    "prefix_is_current_class": true,
    "release_idoc": true,
    "sicf_consistency": true,
    "type_begin_single_include": true,
    "try_without_catch": true,
    "xml_consistency": true,
    "use_new": false,
    "unused_variables": false,
    "unknown_types": false,
    "types_naming": false,
    "type_form_parameters": true,
    "sql_escape_host_variables": false,
    "space_before_dot": true,
    "selection_screen_naming": false,
    "rfc_error_handling": true,
    "prefer_returning_to_exporting": true,
    "newline_between_methods": false,
    "line_break_multiple_parameters": false,
    "fully_type_constants": true,
    "check_syntax": true,
    "7bit_ascii": true,
    "avoid_use": {
      "define": true,
      "endselect": true,
      "execSQL": true,
      "break": true,
      "communication": true,
      "defaultKey": false,
      "systemCall": true,
      "statics": true,
      "kernelCall": true
    },
    "class_attribute_names": true,
    "cloud_types": true,
    "colon_missing_space": true,
    "commented_code": true,
    "constructor_visibility_public": true,
    "contains_tab": true,
    "definitions_top": true,
    "description_empty": true,
    "double_space": true,
    "empty_line_in_statement": true,
    "empty_statement": true,
    "exit_or_check": true,
    "exporting": true,
    "functional_writing": true,
    "global_class": true,
    "identical_form_names": true,
    "if_in_if": true,
    "indentation": true,
    "inline_data_old_versions": true,
    "line_length": {
      "length": 120
    },
    "line_only_punc": true,
    "local_class_naming": true,
    "local_variable_names": {
      "expectedData": "^L._.*$",
      "expectedConstant": "^LC_.*$",
      "expectedFS": "^<L._.*>$"
    },
    "max_one_statement": true,
    "message_exists": true,
    "method_length": true,
    "method_parameter_names": {
      "ignoreExceptions": true,
      "importing": "^I._.*$",
      "returning": "^R._.*$",
      "changing": "^C._.*$",
      "exporting": "^E._.*$",
      "ignoreNames": [
        "P_TASK"
      ]
    },
    "mix_returning": true,
    "msag_consistency": true,
    "nesting": true,
    "no_public_attributes": true,
    "object_naming": {
      "clas": "^ZC(L|X)\\_",
      "intf": "^ZIF\\_",
      "prog": "^Z",
      "fugr": "^Z",
      "tabl": "^Z",
      "ttyp": "^Z",
      "dtel": "^Z",
      "doma": "^Z",
      "msag": "^Z",
      "tran": "^Z",
      "enqu": "^Z",
      "auth": "^Z",
      "ssfo": "^Z",
      "ssst": "^Z",
      "xslt": "^Z",
      "idoc": "^Z",
      "pinf": "^Z"
    },
    "obsolete_statement": true,
    "parser_error": true,
    "preferred_compare_operator": {
      "badOperators": [
        "EQ",
        "><",
        "NE",
        "GE",
        "GT",
        "LT",
        "LE"
      ]
    },
    "remove_descriptions": true,
    "sequential_blank": true,
    "short_case": true,
    "space_before_colon": true,
    "start_at_tab": true,
    "superclass_final": false,
    "tabl_enhancement_category": true,
    "unreachable_code": true,
    "when_others_last": true,
    "whitespace_end": true
  }
}
